<% @title = 'Empreiteiras no Japão' %>
<% provide(:title, @title) %>
<div class='row'>
  <div class='page-header'>
    <h1><%= @title %></h1>
    <h2>Organizadas por província de atuação</h2>
  </div>
  <div class='span12'>
    <ul>
      <% @prefectures.each do |prefecture| %>
      <li><%= link_to prefecture.name, list_companies_path(prefecture.name.downcase) %></li>
      <% end %>
    </ul>
  </div>
</div>